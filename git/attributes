# Define default diff patterns for many built in filetypes
# These are built into git, why not enabled by default? IDK
*.bash  diff=bash
*.c     diff=cpp
*.c++   diff=cpp
*.cc    diff=cpp
*.cpp   diff=cpp
*.cs    diff=csharp
*.css   diff=css
*.el    diff=lisp
*.ex    diff=elixir
*.exs   diff=elixir
*.go    diff=golang
*.h     diff=cpp
*.h++   diff=cpp
*.hh    diff=cpp
*.hpp   diff=cpp
*.html  diff=html
*.ini   diff=ini
*.java  diff=java
*.kt    diff=kotlin
*.kts   diff=kotlin
*.lisp  diff=lisp
*.m     diff=matlab
*.md    diff=markdown
*.php   diff=php
*.php   diff=php
*.pl    diff=perl
*.py    diff=python
*.rake  diff=ruby
*.rb    diff=ruby
*.rs    diff=rust
*.sh    diff=bash
*.tex   diff=text
*.xhtml diff=html
